{
  "name": "vsplugin-helper",
  "displayName": "VSPlugin Helper"
  "version": "0.0.1",
  "description": "一个用于在TraeIDE中自动下载和安装VSCode官方插件市场插件的插件",
  "publisher": "trae-extension",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsplugin-helper.installPlugin"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsplugin-helper.installPlugin",
        "title": "安装VSCode插件"
      }
    ],
    "configuration": {
      "title": "VSPlugin Helper",
      "properties": {
        "vsplugin-helper.traePath": {
          "type": "string",
          "default": "",
          "description": "Trae的安装路径，如果不设置则使用TRAE_HOME环境变量"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "package": "node ./esbuild.js --minify",
    "watch": "node ./esbuild.js --watch",
    "ovsx:publish": "ovsx publish",
    "publish": "node ./scripts/publish.js"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "esbuild": "^0.14.0",
    "typescript": "^4.5.5",
    "ovsx": "^0.8.0"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/vsplugin-helper.git"
  },
  "license": "MIT"
}